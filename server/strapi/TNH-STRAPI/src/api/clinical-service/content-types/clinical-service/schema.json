{
  "kind": "collectionType",
  "collectionName": "clinical_services",
  "info": {
    "singularName": "clinical-service",
    "pluralName": "clinical-services",
    "displayName": "Clinical Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tagline": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "overview": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "LongDescription": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ImageAlt": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Features": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "feature.feature",
      "repeatable": true
    },
    "contact": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "contact-info.contact-info",
      "repeatable": false
    },
    "testimonial": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "testimonial.testimonial",
      "repeatable": false
    },
    "doctors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::doctor.doctor",
      "inversedBy": "clinical_services"
    }
  }
}
